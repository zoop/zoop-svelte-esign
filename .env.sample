# ==========================
# Environment Configuration (Sample)
# ==========================

# API & Analytics Tokens
VITE_API_TOKEN=your_api_token_here
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# App Environment
VITE_ENV=development   # can be development | staging | production

# ==========================
# eSign Provider URLs
# ==========================

# NSDL eSign request URL
VITE_NSDL_ESIGN_REQ_URL=<nsdl_esign_request_url>

# Verasys eSign authentication URL
VITE_VSIGN_AUTH_URL=<verasys_auth_url>

# New Verasys eSign authentication URL
VITE_NEW_VSIGN_AUTH_URL=<new_verasys_auth_url>

# CDSL eSign request URL
VITE_CDSL_ESIGN_REQ_URL=<cdsl_esign_request_url>

# eMudhra eSign request URL
VITE_EMUDHRA_REQ_URL=<emudhra_esign_request_url>
